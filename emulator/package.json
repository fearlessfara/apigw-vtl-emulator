{
  "name": "apigw-vtl-emulator",
  "version": "1.0.0",
  "description": "VTL evaluator using velocityjs, simulating AWS API Gateway mapping template behavior",
  "type": "module",
  "main": "dist/vtl.umd.js",
  "module": "dist/vtl.mjs",
  "exports": {
    "import": "./dist/vtl.mjs",
    "require": "./dist/vtl.umd.js"
  },
  "scripts": {
    "build": "esbuild src/engine.js --bundle --minify --outfile=dist/vtl.mjs --format=esm && esbuild src/engine.js --bundle --minify --outfile=dist/vtl.umd.js --format=iife --global-name=VTL",
    "test": "mocha tests/**/*test.js"
  },
  "keywords": [
    "vtl",
    "velocity",
    "velocityjs",
    "apigateway",
    "apigw",
    "emulator",
    "serverless",
    "mapping-template",
    "aws"
  ],
  "author": "Christian Gennaro Faraone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fearlessfara/apigw-vtl-emulator.git"
  },
  "bugs": {
    "url": "https://github.com/fearlessfara/apigw-vtl-emulator/issues"
  },
  "homepage": "https://github.com/fearlessfara/apigw-vtl-emulator#readme",
  "dependencies": {
    "velocityjs": "~2.1.5"
  },
  "devDependencies": {
    "esbuild": "~0.25.4",
    "chai": "~5.2.0",
    "mocha": "~11.4.0"
  }
}
